{
  "name": "@paperless/core",
  "version": "0.1.0-alpha.27",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/paperless/paperless.esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ionic-team/stencil-component-starter.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "build": "stencil build --docs",
    "build:prod": "stencil build",
    "postbuild": "yarn tailwind:process",
    "postbuild:prod": "yarn tailwind:process",
    "tailwind:process": "tailwindcss -i dist/paperless/paperless.css -o dist/paperless/paperless.css",
    "watch": "stencil build --watch",
    "watch:bg": "(yarn watch&)",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "preyarn:publish": "cp ../../README.md .",
    "yarn:publish": "yarn publish"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.5",
    "@stencil/core": "^2.13.0",
    "rxjs": "^7.5.5"
  },
  "devDependencies": {
    "@stencil/angular-output-target": "^0.4.0",
    "@stencil/postcss": "^2.1.0",
    "@stencil/react-output-target": "^0.3.1",
    "@stencil/sass": "^1.5.2",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@tailwindcss/typography": "^0.5.2",
    "@types/jest": "^27.0.3",
    "jest": "^27.4.5",
    "jest-cli": "^27.4.5",
    "puppeteer": "^10.0.0",
    "stencil-inline-svg": "^1.1.0",
    "stencil-tailwind-plugin": "^1.3.0",
    "tailwindcss": "~3.1.4",
    "workbox-build": "4.3.1"
  },
  "license": "MIT"
}
