{
    "name": "@paperless/angular",
    "version": "0.1.0-alpha.45",
    "scripts": {
        "ng": "ng",
        "set-version": "cd projects/paperless && yarn version --new-version=$(node -p \"require('../../package.json').version\") --no-git-tag-version",
        "start": "ng serve",
        "prebuild": "yarn",
        "build": "ng build",
        "prebuild:prod": "yarn set-version",
        "build:prod": "ng build --prod",
        "watch": "ng build --watch --configuration development",
        "test": "ng test",
        "preyarn:publish": "cp ../../README.md ./dist/",
        "yarn:publish": "yarn publish ./dist  --access=public"
    },
    "dependencies": {
        "@angular/animations": "13.3.11",
        "@angular/common": "13.3.11",
        "@angular/compiler": "13.3.11",
        "@angular/core": "13.3.11",
        "@angular/forms": "13.3.11",
        "@angular/platform-browser": "13.3.11",
        "@angular/platform-browser-dynamic": "13.3.11",
        "@angular/router": "13.3.11",
        "rxjs": "7.5.5",
        "tslib": "2.4.0",
        "zone.js": "0.11.6"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "13.3.8",
        "@angular/cli": "13.3.8",
        "@angular/compiler-cli": "13.3.11",
        "@types/jasmine": "3.10.6",
        "@types/node": "12.20.55",
        "jasmine-core": "4.0.1",
        "karma": "6.3.20",
        "karma-chrome-launcher": "3.1.1",
        "karma-coverage": "2.1.1",
        "karma-jasmine": "4.0.2",
        "karma-jasmine-html-reporter": "2.0.0",
        "ng-packagr": "13.3.1",
        "typescript": "4.6.4"
    }
}
